# Private key for deployment (DO NOT COMMIT YOUR ACTUAL PRIVATE KEY)
PRIVATE_KEY=your_private_key_here

# Infura API Key (get from https://infura.io)
INFURA_API_KEY=your_infura_api_key

# Etherscan API Key for contract verification (get from https://etherscan.io)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Optional: Other network explorers
OPTIMISTIC_ETHERSCAN_API_KEY=your_optimistic_etherscan_key
ARBISCAN_API_KEY=your_arbiscan_key
POLYGONSCAN_API_KEY=your_polygonscan_key
BASESCAN_API_KEY=your_basescan_key

# Gas reporting
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_key
